{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "ApplicationInsights": "InstrumentationKey=xxxxxxxxxxxxxxxxxxx;IngestionEndpoint=https://westeurope-5.in.applicationinsights.azure.com/;LiveEndpoint=https://westeurope.livediagnostics.monitor.azure.com/"
  },
  "app": {
    "name": "Telegram-Integration WebApi",
    "service": "telegram-integration-service",
    "instance": "000001",
    "version": "v1.0",
    "displayBanner": true,
    "displayVersion": true
  },
  "logger": {
    "level": "warning",
    "excludePaths": [
      "/",
      "/ping",
      "/metrics"
    ],
    "excludeProperties": [
      "api_key",
      "access_key",
      "ApiKey",
      "ApiSecret",
      "ClientId",
      "ClientSecret",
      "ConnectionString",
      "Password",
      "Email",
      "Login",
      "Secret",
      "Token"
    ],
    "console": {
      "enabled": true
    },
    "elk": {
      "enabled": false,
      "url": "http://localhost:9200",
      "basicAuthEnabled": false,
      "username": null,
      "password": null,
      "indexFormat": null
    },
    "file": {
      "enabled": true,
      "path": "logs/logs.txt",
      "interval": "day"
    },
    "seq": {
      "enabled": false,
      "url": "http://localhost:5341",
      "apiKey": "secret"
    },
    "loki": {
      "enabled": false,
      "url": "http://localhost:5341",
      "batchPostingLimit": 1000,
      "queueLimit": 100000,
      "period": "00:00:01",
      "lokiUsername": "admin",
      "lokiPassword": "password"
    },
    "azure": {
      "enabled": false,
      "ConnectionString": null
    },
    "tags": {}
  },
  "metrics": {
    "enabled": true,
    "influxEnabled": false,
    "prometheusEnabled": false,
    "prometheusFormatter": null,
    "influxUrl": "http://localhost:8086",
    "database": "test",
    "interval": 5
  },
  "telegram": {
    "Token": null
  },
  "mongoDb": {
    "ConnectionString": "mongodb://localhost",
    "Database": "Telegram",
    "EnableTracing": true
  },
  "rabbitMq": {
    "hostName": "localhost"
  },
  "openAI": {
    "APIKey": null,
    "Url": "https://api.openai.com/v1/completions"
  },
  "openTelemetry": {
    "AgentHost": "localhost"
  },
  "apiClient": {
    "FormRecognizerUrl": "https://api.telegram.org/bot"
  },
  "azureCognitiveServices": {
    "Endpoint": null,
    "SubscriptionKey": null
  },
  "azureStorage": {
    "AccountName": null,
    "AccountKey": null,
    "UploadContainer": null,
    "TrainingSetContainerUrl": null,
    "ThumbnailContainer": null,
    "InspectingFileUrl": null
  },
  "imageClassifier": {
    "Endpoint": null,
    "PredictionKey": null,
    "ModelId": null
  },
  "formRecognizer": {
    "Endpoint": null,
    "PredictionKey": null
  },
  "stripe": {
    "token": null
  },
  "AppSettings": {
    "ServiceName": "Telegram WebApi"
  }
}