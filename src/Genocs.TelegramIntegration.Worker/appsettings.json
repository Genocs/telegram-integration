{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Information",
      "Microsoft": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "app": {
    "name": "Telegram-Integration Worker",
    "service": "telegram-integration-worker",
    "instance": "000001",
    "version": "v1.0",
    "displayBanner": true,
    "displayVersion": true
  },
  "consul": {
    "enabled": false,
    "url": "http://localhost:8500",
    "service": "telegram-service",
    "address": "docker.for.win.localhost",
    "port": "5002",
    "pingEnabled": true,
    "pingEndpoint": "ping",
    "pingInterval": 3,
    "removeAfterInterval": 3
  },
  "fabio": {
    "enabled": false,
    "url": "http://localhost:9999",
    "service": "telegram-service"
  },
  "httpClient": {
    "type": "",
    "retries": 2,
    "services": {
    }
  },
  "logger": {
    "applicationName": "telegram-service",
    "excludePaths": [ "/ping", "/metrics" ],
    "level": "information",
    "console": {
      "enabled": true
    },
    "file": {
      "enabled": true,
      "path": "logs/logs.txt",
      "interval": "day"
    },
    "seq": {
      "enabled": true,
      "url": "http://localhost:5341",
      "token": "secret"
    }
  },
  "jaeger": {
    "enabled": true,
    "serviceName": "telegram",
    "udpHost": "localhost",
    "udpPort": 6831,
    "maxPacketSize": 0,
    "sampler": "const",
    "excludePaths": [ "/ping", "/metrics" ]
  },
  "metrics": {
    "enabled": true,
    "influxEnabled": false,
    "prometheusEnabled": true,
    "influxUrl": "http://localhost:8086",
    "database": "test",
    "env": "local",
    "interval": 5
  },
  "mongo": {
    "connectionString": "mongodb://localhost:27017",
    "database": "telegram-service",
    "EnableTracing": "true",
    "seed": false
  },
  "mongodb": {
    "connectionString": "mongodb://localhost:27017",
    "database": "telegram-service",
    "EnableTracing": "true",
    "seed": false
  },
  "prometheus": {
    "enabled": false
  },
  "rabbitMq": {
    "connectionName": "telegram-service",
    "messagesPersisted": true,
    "retries": 3,
    "retryInterval": 2,
    "conventionsCasing": "snakeCase",
    "logger": {
      "enabled": true,
      "logConnectionStatus": true,
      "logMessagePayload": true
    },
    "hostname": "localhost",
    "username": "guest",
    "password": "guest",
    "virtualHost": "/",
    "port": 5672,
    "hostnames": [
      "localhost",
      "rabbitmq"
    ],
    "requestedConnectionTimeout": "00:00:30",
    "requestedHeartbeat": "00:01:00",
    "socketReadTimeout": "00:00:30",
    "socketWriteTimeout": "00:00:30",
    "continuationTimeout": "00:00:20",
    "handshakeContinuationTimeout": "00:00:10",
    "networkRecoveryInterval": "00:00:05",
    "exchange": {
      "declare": true,
      "durable": true,
      "autoDelete": false,
      "type": "topic",
      "name": "orders"
    },
    "queue": {
      "declare": true,
      "durable": true,
      "exclusive": false,
      "autoDelete": false,
      "template": "{{assembly}}/{{exchange}}.{{message}}"
    },
    "context": {
      "enabled": true,
      "header": "message_context"
    },
    "spanContextHeader": "span_context",
    "deadLetter": {
      "enabled": true,
      "prefix": "dlx-",
      "declare": true
    },
    "maxProducerChannels": 1000
  },
  "swagger": {
    "enabled": true,
    "reDocEnabled": false,
    "name": "v1",
    "title": "Telegram Service",
    "version": "v1",
    "routePrefix": "swagger"
  },
  "redis": {
    "connectionString": "localhost",
    "instance": "telegram:"
  },
  "security": {
    "certificate": {
      "enabled": true,
      "header": "Certificate",
      "allowedHosts": [ "localhost", "product" ]
    }
  },
  "vault": {
    "enabled": true,
    "url": "http://localhost:8200",
    "authType": "token",
    "token": "secret",
    "username": "user",
    "password": "secret",
    "kv": {
      "enabled": false,
      "engineVersion": 2,
      "mountPoint": "kv",
      "path": "telegram-service/settings"
    },
    "pki": {
      "enabled": false,
      "roleName": "telegram-service",
      "commonName": "telegram-service.convey.io"
    },
    "lease": {
      "mongo": {
        "type": "database",
        "roleName": "telegram-service",
        "enabled": false,
        "autoRenewal": true,
        "templates": {
          "connectionString": "mongodb://{{username}}:{{password}}@localhost:27017"
        }
      }
    }
  },
  "webApi": {
    "bindRequestFromRoute": true
  },
  "ConnectionStrings": {
    "ApplicationInsights": ""
  },
  "AppSettings": {
    "ServiceName": "Telegram Worker"
  },
  "Telegram": {
    "token": null
  },
  "OpenAI": {
    "aPIKey": null,
    "Url": "https://api.openai.com/v1/completions"
  },
  "OpenTelemetry": {
    "AgentHost": "localhost"
  },
  "Monitoring": {
    "Jaeger": "localhost"
  },
  "ApiClient": {
    "FormRecognizerUrl": "https://api.telegram.org/bot"
  }
}
