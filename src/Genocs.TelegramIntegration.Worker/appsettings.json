{
  "app": {
    "name": "Telegram-Integration Worker",
    "service": "telegram-integration-worker",
    "instance": "01",
    "version": "v1.0",
    "displayBanner": true,
    "displayVersion": true
  },
  "logger": {
    "level": "warning",
    "applicationName": "telegram-worker",
    "excludePaths": [
      "/",
      "/ping",
      "/metrics"
    ],
    "excludeProperties": [
      "api_key",
      "access_key",
      "ApiKey",
      "ApiSecret",
      "ClientId",
      "ClientSecret",
      "ConnectionString",
      "Password",
      "Email",
      "Login",
      "Secret",
      "Token"
    ],
    "console": {
      "enabled": true
    },
    "file": {
      "enabled": true,
      "path": "logs/logs.txt",
      "interval": "day"
    },
    "azure": {
      "enabled": false,
      "ConnectionString": null
    },
    "tags": {}
  },
  "metrics": {
    "enabled": true,
    "influxEnabled": false,
    "prometheusEnabled": false,
    "prometheusFormatter": null,
    "influxUrl": "http://localhost:8086",
    "database": "test",
    "interval": 5
  },
  "jaeger": {
    "enabled": true,
    "serviceName": "telegram-worker",
    "udpHost": "localhost",
    "udpPort": 6831,
    "maxPacketSize": 0,
    "sampler": "const",
    "excludePaths": [ "/ping", "/metrics" ]
  },
  "mongodb": {
    "connectionString": "mongodb://localhost:27017",
    "database": "telegram-service",
    "EnableTracing": true,
    "seed": false
  },
  "rabbitMq": {
    "connectionName": "telegram-worker",
    "messagesPersisted": true,
    "retries": 3,
    "retryInterval": 2,
    "conventionsCasing": "snakeCase",
    "logger": {
      "enabled": true,
      "logConnectionStatus": true,
      "logMessagePayload": true
    },
    "hostname": "localhost",
    "username": "guest",
    "password": "guest",
    "virtualHost": "/",
    "port": 5672,
    "hostnames": [
      "localhost",
      "rabbitmq"
    ],
    "requestedConnectionTimeout": "00:00:30",
    "requestedHeartbeat": "00:01:00",
    "socketReadTimeout": "00:00:30",
    "socketWriteTimeout": "00:00:30",
    "continuationTimeout": "00:00:20",
    "handshakeContinuationTimeout": "00:00:10",
    "networkRecoveryInterval": "00:00:05",
    "exchange": {
      "declare": true,
      "durable": true,
      "autoDelete": false,
      "type": "topic",
      "name": "orders"
    },
    "queue": {
      "declare": true,
      "durable": true,
      "exclusive": false,
      "autoDelete": false,
      "template": "{{assembly}}/{{exchange}}.{{message}}"
    },
    "context": {
      "enabled": true,
      "header": "message_context"
    },
    "spanContextHeader": "span_context",
    "deadLetter": {
      "enabled": true,
      "prefix": "dlx-",
      "declare": true
    },
    "maxProducerChannels": 1000
  },
  "webApi": {
    "bindRequestFromRoute": true
  },
  "telegram": {
    "token": null
  },
  "openAI": {
    "aPIKey": null,
    "Url": "https://api.openai.com/v1/completions"
  },
  "apiClient": {
    "FormRecognizerUrl": "https://localhost:5201/ScanForm/ClassifyAndEvaluate"
  }
}